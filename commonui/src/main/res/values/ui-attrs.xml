<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseElement">
        <attr name="mainText" format="string"/>
        <attr name="additionalText" format="string"/>
        <attr name="size" format="enum">
            <enum name="big" value="0" />
            <enum name="small" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ImageElement" parent="BaseElement">
        <attr name="iconSource" format="reference"/>
    </declare-styleable>
    <attr name="textAppearanceButtonItalic" format="reference" />
    <attr name="colorRipple" format="color"/>
    <attr name="boxColor" format="color"/>
    <attr name="baseTextAppearance" format="reference"/>
</resources>